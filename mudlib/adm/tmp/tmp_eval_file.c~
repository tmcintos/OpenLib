#include <mudlib.h>
#include <daemons.h>
#include <net/daemons.h>

void
preeval()
{
  unguarded((: set_privs(this_object(),
			 query_privs(this_interactive())) :), 1);
}

mixed
eval()
{
  object me = this_player();
  mapping* x = values(TERMCAP_D->query_all_termcap_entries(); mapping ret = ([]); foreach(mapping tc in x) ret += tx; write_file("/tmp/tc", dump_variable(ret));;
}
